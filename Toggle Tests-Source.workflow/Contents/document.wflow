				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run	tell application "Finder" to set windowTarget to the target of the front Finder window as alias	set posixPath to POSIX path of windowTarget	set phpString to "$dir =trim(\"" &amp; posixPath &amp; "\");$newDir = preg_replace(\"/Test/\", \"\", $dir);if ($dir != $newDir) {	if (!file_exists($newDir)) {		mkdir($newDir, 0777, true);	}	echo $newDir;} else {	$dirParts = explode(\"/\", $dir);	for ($i=0; $i &lt; count($dirParts); $i++) { 		$tempDir = \"\";		for ($j=1; $j &lt;= $i; $j++) { 			$tempDir .= \"/\".$dirParts[$j];		}		$tempDir .= \"Test\";		if (file_exists($tempDir)) {			$dirParts[$i] = $dirParts[$i].\"Test\";			$newDir = implode(\"/\", $dirParts);			if (!file_exists($newDir)) {			    mkdir($newDir, 0777, true);			}			echo $newDir;			exit;		}	}	echo -1;}	"	set commandString to "php -r '" &amp; phpString &amp; "'"	set phpResult to do shell script commandString	#display dialog phpResult	if (phpResult is "-1") then		display dialog "No Test Folder found."	else		tell application "Finder"			set the target of the front Finder window to (POSIX file phpResult)		end tell	end ifend run</string>
